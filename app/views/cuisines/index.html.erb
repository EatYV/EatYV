<p id="notice"><%= notice %></p>

<h1>Cuisines</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cuisines.each do |cuisine| %>
      <tr>
        <td><%= link_to cuisine.title, cuisine %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<% if chef_signed_in? %>
  <%= link_to 'New Cuisine', new_cuisine_path %>
<% end %>
